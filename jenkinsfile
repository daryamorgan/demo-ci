pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
                echo ${env.GIT_COMMIT}
                echo $ {env.GIT_BRANCH}
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
